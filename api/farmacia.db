CREATE TABLE `producto` (
  `id` int NOT NULL AUTO_INCREMENT,
  `nombre` varchar(45) NOT NULL,
  `codigo` int NOT NULL,
  `precio` float NOT NULL,
  `stock` int NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=9 DEFAULT CHARSET=utf8mb3


CREATE TABLE `lote` (
  `id` int NOT NULL AUTO_INCREMENT,
  `lote` varchar(45) NOT NULL,
  `vencimiento` datetime NOT NULL,
  `cantidad` int NOT NULL,
  `producto_id` int NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_Lote_Producto1_idx` (`producto_id`),
  CONSTRAINT `fk_Lote_Producto1` FOREIGN KEY (`producto_id`) REFERENCES `producto` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=19 DEFAULT CHARSET=utf8mb3


CREATE TABLE `compra` (
  `id` int NOT NULL AUTO_INCREMENT,
  `fecha` datetime NOT NULL,
  `proveedor` varchar(45) NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=5 DEFAULT CHARSET=utf8mb3


CREATE TABLE `detallecompra` (
  `id` int NOT NULL AUTO_INCREMENT,
  `cantidad` int NOT NULL,
  `precio` float NOT NULL,
  `producto_id` int NOT NULL,
  `compra_id` int NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_DetalleCompra_Producto1_idx` (`producto_id`),
  KEY `fk_DetalleCompra_Venta1_idx` (`compra_id`),
  CONSTRAINT `fk_DetalleCompra_Producto1` FOREIGN KEY (`producto_id`) REFERENCES `producto` (`id`),
  CONSTRAINT `fk_DetalleCompra_Venta1` FOREIGN KEY (`compra_id`) REFERENCES `compra` (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=13 DEFAULT CHARSET=utf8mb3


CREATE TABLE `empleado` (
  `id` int NOT NULL AUTO_INCREMENT,
  `nombre` varchar(45) NOT NULL,
  `rol` varchar(45) NOT NULL,
  `contrasena` int NOT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3



CREATE TABLE `venta` (
  `id` int NOT NULL AUTO_INCREMENT,
  `fecha` datetime NOT NULL,
  `cliente` varchar(45) NOT NULL,
  `empleado_id` int NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_CompraVenta_Empleado_idx` (`empleado_id`),
  CONSTRAINT `fk_CompraVenta_Empleado` FOREIGN KEY (`empleado_id`) REFERENCES `empleado` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3


CREATE TABLE `detalleventa` (
  `id` int NOT NULL AUTO_INCREMENT,
  `cantidad` int NOT NULL,
  `precio` float NOT NULL,
  `compraVenta_id` int NOT NULL,
  `producto_id` int NOT NULL,
  PRIMARY KEY (`id`),
  KEY `fk_Detalle_CompraVenta1_idx` (`compraVenta_id`),
  KEY `fk_Detalle_Producto1_idx` (`producto_id`),
  CONSTRAINT `fk_Detalle_CompraVenta1` FOREIGN KEY (`compraVenta_id`) REFERENCES `venta` (`id`),
  CONSTRAINT `fk_Detalle_Producto1` FOREIGN KEY (`producto_id`) REFERENCES `producto` (`id`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb3
